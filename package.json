{
  "name": "lasvegashomeexpert.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check .",
    "fix": "biome check . --write",
    "fix:unsafe": "biome check . --write --unsafe",
    "type-check": "tsc --noEmit",
    "code-quality": "npm run lint && npm run type-check",
    "pre-commit": "npm run format && npm run code-quality",
    "biome:ci": "biome ci .",
    "biome:check:verbose": "biome check . --verbose",
    "biome:stats": "biome check . --verbose --max-diagnostics=1000",
    "prepare": "husky"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "next": "^13.5.6",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-intersection-observer": "^9.5.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "husky": "^9.1.7"
  }
}
